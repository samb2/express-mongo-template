{
  "name": "irole-express-mongo-template",
  "version": "1.0.0",
  "description": "this is template for api with mongodb",
  "main": "src/api/bin",
  "scripts": {
    "tsc": "tsc",
    "start": "cross-env NODE_ENV=production ts-node --files src/api/bin",
    "start-dev": "cross-env NODE_ENV=development tsnd --poll --files src/api/bin",
    "start-test": "cross-env NODE_ENV=test tsnd --files src/api/bin",
    "jest": "jest",
    "jest-cover": "jest --coverage",
    "migrate-up": "npx migrate up",
    "migrate-down": "npx migrate down {$migrate-name}",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samb2/irole-express-mongo-template.git"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "microservice",
    "mongoDB",
    "mongoose",
    "template",
    "boilerplate",
    "redis",
    "i18n",
    "passport",
    "jest",
    "swagger"
  ],
  "author": "samb2 and mohamad_ka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samb2/irole-express-mongo-template/issues"
  },
  "homepage": "https://github.com/samb2/irole-express-mongo-template#readme",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/i18n": "^0.13.6",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/uuid": "^9.0.0",
    "auto-bind": "^4.0.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "core-decorators": "^0.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jsdoc-swagger": "^1.8.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "i18n": "^0.15.1",
    "irolegroup": "^1.0.5",
    "jsonwebtoken": "^9.0.0",
